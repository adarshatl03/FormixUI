{
  "name": "formix-ui",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "description": "The complete UI framework for building form-driven React applications with visual builder, CLI tools, and enterprise-grade features",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib",
    "prepare": "husky",
    "predeploy": "npm run build",
    "build:site": "vite build",
    "build:lib": "vite build --config vite.lib.config.ts && tsc -p tsconfig.lib.json && npm run build:cli",
    "build:cli": "tsc -p tsconfig.cli.json",
    "deploy": "gh-pages -d dist-site",
    "update-status": "node .agent/scripts/generate-status.js"
  },
  "bin": {
    "formix-ui": "dist/bin/index.js",
    "create-formix-ui": "dist/bin/index.js"
  },
  "keywords": [
    "react",
    "form",
    "form-builder",
    "schema",
    "validation",
    "typescript",
    "tailwind",
    "dynamic-forms",
    "formix",
    "ui-framework",
    "zod",
    "yup",
    "formik",
    "visual-builder",
    "drag-and-drop",
    "conditional-logic",
    "responsive",
    "accessibility",
    "dark-mode",
    "cli"
  ],
  "author": "Adarsh <adarshatl03@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adarshatl03/FormixUI.git"
  },
  "bugs": {
    "url": "https://github.com/adarshatl03/FormixUI/issues"
  },
  "homepage": "https://adarshatl03.github.io/FormixUI/",
  "peerDependencies": {
    "formik": ">=2.4.0",
    "lucide-react": ">=0.1.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-router-dom": ">=6.0.0",
    "tailwindcss": ">=4.0.0",
    "yup": ">=1.0.0",
    "zod": ">=3.0.0"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "date-fns": "^4.1.0",
    "execa": "^9.6.1",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.7",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "formik": "^2.4.9",
    "fs-extra": "^11.3.3",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "lucide-react": "^0.562.0",
    "prettier": "^3.7.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.12.0",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "yup": "^1.7.1",
    "zod": "^3.24.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
